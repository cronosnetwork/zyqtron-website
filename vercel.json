{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {"src": "/$", "dest": "/index.html"},
    {"src": "/contact$", "dest": "/contact.html"},
    {"src": "/legal$", "dest": "/legal.html"},
    {"src": "/apropos$", "dest": "/apropos.html"},
    {"src": "/solutions$", "dest": "/solutions.html"},
    {"src": "/analyse$", "dest": "/analyse.html"},
    {"src": "/analyse-arnaque$", "dest": "/analyse-arnaque.html"},
    {"src": "/audit-web$", "dest": "/audit-web.html"},
    {"src": "/creation$", "dest": "/creation.html"},
    {"src": "/contrat$", "dest": "/contrat.html"},
    {"src": "/produit$", "dest": "/produit.html"},
    {"src": "/rapport$", "dest": "/rapport.html"},
    {"src": "/rayon$", "dest": "/rayon.html"},
    {"src": "/reputation$", "dest": "/reputation.html"},
    {"src": "/achat$", "dest": "/achat.html"},
    {"handle": "filesystem"},
    {"src": "/(.*)", "dest": "/$1"}
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {"key": "X-Content-Type-Options", "value": "nosniff"},
        {"key": "X-Frame-Options", "value": "DENY"},
        {"key": "Referrer-Policy", "value": "strict-origin-when-cross-origin"},
        {"key": "Permissions-Policy", "value": "camera=(), microphone=(), geolocation=()"}
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {"key": "Cache-Control", "value": "public, max-age=31536000, immutable"}
      ]
    }
  ]
}
